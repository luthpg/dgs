{
  "compilerOptions": {
    "target": "es2016",
    "module": "ESNext",
    "types": ["@types/node", "@types/google-apps-script", "vitest/globals"],
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}
